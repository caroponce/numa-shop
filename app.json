{
  "expo": {
    "name": "numashop",
    "slug": "numashop",
    "version": "1.0.0",
    "icon": "./assets/IconNuma.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/IconNuma.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.numashop.numashop"
    },
    "plugins" : [
      [
        "expo-screen-orientation",
        {
          "initialOrientation" : "DEFAULT"
        }
      ],
      [
        "expo-image-picker",
        {
          "photoPermission": "The app access your photos to let your shares with your contacts"
        }
      ]
      ],
      
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/IconNuma.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.numashop.numashop"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
